<thead>
  <tr>
    <th>
      <%= render TableCheckboxComponent.new(id: 'all', resource: resources), cached: true %>
    </th>

    <% labels.each do |label| %>
      <% cache label do %>
        <%= render TableHeaderCellComponent.new(label: label), cached: true %>
      <% end %>
    <% end %>

    <th scope='col'
        colspan="<%= colspan %>">
    </th>
  </tr>
</thead>
